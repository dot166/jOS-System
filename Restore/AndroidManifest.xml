<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.j.jOSRestore"
    coreApp="true"
    android:sharedUserId="android.uid.system">

    <queries>
        <package android:name="com.google.android.apps.restore" />
        <package android:name="org.stevesoltys.seedvault" />
    </queries>

    <application
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@*lineageos.platform:style/jOS.Theme">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="com.j.jOSRestore.RESTORE_LAUNCHER"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
    </application>

</manifest>
