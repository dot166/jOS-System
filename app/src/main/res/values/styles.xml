<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="MyCardView" parent="Widget.Material3.CardView.Outlined">
        <item name="cornerRadius">12dp</item>
    </style>

    <style name="LinkChipButton" parent="Widget.Material3.Button.TextButton">
        <item name="android:textStyle">bold</item>
        <item name="android:checkable">false</item>
        <item name="android:clickable">true</item>
        <item name="android:focusable">true</item>
        <item name="android:minWidth">0dp</item>
        <item name="android:enabled">true</item>
        <item name="iconSize">24dp</item>
        <item name="android:textColor">?attr/colorOnSurface</item>
    </style>

    <style name="jOSButton" parent="@style/LinkChipButton"/>
    <style name="BottomAppBar" parent="Widget.Material3.BottomAppBar">
    <item name="theme">@style/BottomAppBar.Theme</item>
    <item name="navigationIconTint">?attr/colorOnSurface</item>
</style>

    <style name="BottomAppBar.Theme" parent="ThemeOverlay.Material3.BottomAppBar">
    </style>
</resources>