<?xml version="1.0" encoding="utf-8"?>

<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/coordinatorLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <androidx.core.widget.NestedScrollView
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <androidx.cardview.widget.CardView
                        android:id="@+id/ics"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginEnd="8dp"
                        android:layout_marginBottom="8dp">

                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:paddingBottom="8dp">

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent">

                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="#333333"
                                    android:padding="8dp">

                                    <LinearLayout
                                        android:id="@+id/layoutDescription1"
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="16dp"
                                        android:gravity="center_vertical"
                                        android:minHeight="40dp"
                                        android:orientation="vertical"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toEndOf="@+id/imageView1"
                                        app:layout_constraintTop_toTopOf="parent">

                                        <TextView
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:text="@string/i_egg_name"
                                            android:textAppearance="@android:style/TextAppearance.Holo.Large"
                                            android:textColor="#FFFFFF"
                                            tools:text="Android Easter Egg name" />

                                        <TextView
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:text="@string/i_egg"
                                            android:textAppearance="@android:style/TextAppearance.Holo.Medium"
                                            tools:text="Android Version"
                                            tools:visibility="visible" />
                                    </LinearLayout>

                                    <ImageView
                                        android:id="@+id/imageView1"
                                        android:layout_width="40dp"
                                        android:layout_height="40dp"
                                        android:layout_marginStart="8dp"
                                        android:padding="4dp"
                                        android:src="@drawable/i_platlogo"
                                        app:layout_constraintBottom_toBottomOf="@id/layoutDescription1"
                                        app:layout_constraintEnd_toStartOf="@+id/layoutDescription1"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toTopOf="@id/layoutDescription1"
                                        app:layout_constraintVertical_bias="0.0"
                                        tools:srcCompat="@mipmap/apk"
                                        tools:visibility="visible" />

                                </androidx.constraintlayout.widget.ConstraintLayout>
                            </LinearLayout>
                        </androidx.constraintlayout.widget.ConstraintLayout>
                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView
                        android:id="@+id/k"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginEnd="8dp"
                        android:layout_marginBottom="8dp">

                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:paddingBottom="8dp">

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent">

                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="#333333"
                                    android:padding="8dp">

                                    <LinearLayout
                                        android:id="@+id/layoutDescription2"
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="16dp"
                                        android:gravity="center_vertical"
                                        android:minHeight="40dp"
                                        android:orientation="vertical"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toEndOf="@+id/imageView2"
                                        app:layout_constraintTop_toTopOf="parent">

                                        <TextView
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:text="@string/k_dessert_case"
                                            android:textAppearance="@android:style/TextAppearance.Holo.Large"
                                            android:textColor="#FFFFFF"
                                            tools:text="Android Easter Egg name" />

                                        <TextView
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:text="@string/k_egg"
                                            android:textAppearance="@android:style/TextAppearance.Holo.Medium"
                                            tools:text="Android Version"
                                            tools:visibility="visible" />
                                    </LinearLayout>

                                    <ImageView
                                        android:id="@+id/imageView2"
                                        android:layout_width="40dp"
                                        android:layout_height="40dp"
                                        android:layout_marginStart="8dp"
                                        android:padding="4dp"
                                        android:src="@drawable/k_platlogo"
                                        app:layout_constraintBottom_toBottomOf="@id/layoutDescription2"
                                        app:layout_constraintEnd_toStartOf="@+id/layoutDescription2"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toTopOf="@id/layoutDescription2"
                                        app:layout_constraintVertical_bias="0.0"
                                        tools:srcCompat="@mipmap/apk"
                                        tools:visibility="visible" />

                                </androidx.constraintlayout.widget.ConstraintLayout>
                            </LinearLayout>
                        </androidx.constraintlayout.widget.ConstraintLayout>
                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView
                        android:id="@+id/l"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginEnd="8dp"
                        android:layout_marginBottom="8dp">

                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:paddingBottom="8dp">

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent">

                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="#333333"
                                    android:padding="8dp">

                                    <LinearLayout
                                        android:id="@+id/layoutDescription3"
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="16dp"
                                        android:gravity="center_vertical"
                                        android:minHeight="40dp"
                                        android:orientation="vertical"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toEndOf="@+id/imageView3"
                                        app:layout_constraintTop_toTopOf="parent">

                                        <TextView
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:text="@string/l_lland"
                                            android:textAppearance="@android:style/TextAppearance.Holo.Large"
                                            android:textColor="#FFFFFF"
                                            tools:text="Android Easter Egg name" />

                                        <TextView
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:text="@string/l_egg"
                                            android:textAppearance="@android:style/TextAppearance.Holo.Medium"
                                            tools:text="Android Version"
                                            tools:visibility="visible" />
                                    </LinearLayout>

                                    <ImageView
                                        android:id="@+id/imageView3"
                                        android:layout_width="40dp"
                                        android:layout_height="40dp"
                                        android:layout_marginStart="8dp"
                                        android:padding="4dp"
                                        android:src="@drawable/l_icon"
                                        app:layout_constraintBottom_toBottomOf="@id/layoutDescription3"
                                        app:layout_constraintEnd_toStartOf="@+id/layoutDescription3"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toTopOf="@id/layoutDescription3"
                                        app:layout_constraintVertical_bias="0.0"
                                        tools:srcCompat="@mipmap/apk"
                                        tools:visibility="visible" />

                                </androidx.constraintlayout.widget.ConstraintLayout>
                            </LinearLayout>
                        </androidx.constraintlayout.widget.ConstraintLayout>
                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView
                        android:id="@+id/p"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginEnd="8dp"
                        android:layout_marginBottom="8dp">

                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:paddingBottom="8dp">

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent">

                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="#333333"
                                    android:padding="8dp">

                                    <LinearLayout
                                        android:id="@+id/layoutDescription4"
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="16dp"
                                        android:gravity="center_vertical"
                                        android:minHeight="40dp"
                                        android:orientation="vertical"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toEndOf="@+id/imageView4"
                                        app:layout_constraintTop_toTopOf="parent">

                                        <TextView
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:text="@string/p_app_name"
                                            android:textAppearance="@android:style/TextAppearance.Holo.Large"
                                            android:textColor="#FFFFFF"
                                            tools:text="Android Easter Egg name" />

                                        <TextView
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:text="@string/p_egg"
                                            android:textAppearance="@android:style/TextAppearance.Holo.Medium"
                                            tools:text="Android Version"
                                            tools:visibility="visible" />
                                    </LinearLayout>

                                    <ImageView
                                        android:id="@+id/imageView4"
                                        android:layout_width="40dp"
                                        android:layout_height="40dp"
                                        android:layout_marginStart="8dp"
                                        android:padding="4dp"
                                        android:src="@drawable/p_icon"
                                        app:layout_constraintBottom_toBottomOf="@id/layoutDescription4"
                                        app:layout_constraintEnd_toStartOf="@+id/layoutDescription4"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toTopOf="@id/layoutDescription4"
                                        app:layout_constraintVertical_bias="0.0"
                                        tools:srcCompat="@mipmap/apk"
                                        tools:visibility="visible" />

                                </androidx.constraintlayout.widget.ConstraintLayout>
                            </LinearLayout>
                        </androidx.constraintlayout.widget.ConstraintLayout>
                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView
                        android:id="@+id/q"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginEnd="8dp"
                        android:layout_marginBottom="8dp">

                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:paddingBottom="8dp">

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent">

                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="#333333"
                                    android:padding="8dp">

                                    <LinearLayout
                                        android:id="@+id/layoutDescription5"
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="16dp"
                                        android:gravity="center_vertical"
                                        android:minHeight="40dp"
                                        android:orientation="vertical"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toEndOf="@+id/imageView5"
                                        app:layout_constraintTop_toTopOf="parent">

                                        <TextView
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:text="@string/q_egg_name"
                                            android:textAppearance="@android:style/TextAppearance.Holo.Large"
                                            android:textColor="#FFFFFF"
                                            tools:text="Android Easter Egg name" />

                                        <TextView
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:text="@string/q_egg"
                                            android:textAppearance="@android:style/TextAppearance.Holo.Medium"
                                            tools:text="Android Version"
                                            tools:visibility="visible" />
                                    </LinearLayout>

                                    <ImageView
                                        android:id="@+id/imageView5"
                                        android:layout_width="40dp"
                                        android:layout_height="40dp"
                                        android:layout_marginStart="8dp"
                                        android:padding="4dp"
                                        android:src="@drawable/q_icon"
                                        app:layout_constraintBottom_toBottomOf="@id/layoutDescription5"
                                        app:layout_constraintEnd_toStartOf="@+id/layoutDescription5"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toTopOf="@id/layoutDescription5"
                                        app:layout_constraintVertical_bias="0.0"
                                        tools:srcCompat="@mipmap/apk"
                                        tools:visibility="visible" />

                                </androidx.constraintlayout.widget.ConstraintLayout>
                            </LinearLayout>
                        </androidx.constraintlayout.widget.ConstraintLayout>
                    </androidx.cardview.widget.CardView>
                </LinearLayout>
            </androidx.core.widget.NestedScrollView>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.coordinatorlayout.widget.CoordinatorLayout>
